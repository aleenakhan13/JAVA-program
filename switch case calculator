import java.util.Scanner;
class Calculate
{
 public static void main(String a[])
 { int x,y,z;


Scanner Sc=new Scanner(System.in);
 x=Sc.nextInt();
 y=Sc.nextInt();
 z=Sc.nextInt();
 



 
 switch(z)
{
        case 1:
            System.out.println(x+y);
            break;
        case 2:
             System.out.println(x+y);
            break;
        case 3:
             System.out.println(x+y);
            break;
        case 4:
             System.out.println(x+y);
            break;
      default :
                
        System.out.println("invalid selection");
}
        
    }
    
}
