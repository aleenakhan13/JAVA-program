import java.util.Scanner;
class Table {
public static void main(String[] args) {
Scanner scan = new Scanner(System.in);
int num = 0;
System.out.println("Enter a Positive Integer (> 0) : ");
num = scan.nextInt();
for(int i = 1; i <= 10; i++) {
int t = num * i;
System.out.println(num + " * " + i + " = " + t);
}
} }
